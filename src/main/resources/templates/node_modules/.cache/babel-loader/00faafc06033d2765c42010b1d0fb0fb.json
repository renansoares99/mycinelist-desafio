blique Sobre o Filme</h1>\r\n    <b-form @submit.prevent=\"saveFilme()\">\r\n      <!-- Inicio Input Nome -->\r\n      <label for=\"nome\">Nome</label>\r\n      <b-form-input\r\n        id=\"nome\"\r\n        v-model=\"filme.nome\"\r\n        type=\"text\"\r\n        placeholder=\"Doutor Estranho, ...\"\r\n        required\r\n        autocomplete=\"off\"\r\n        class=\"mb-2\"\r\n      ></b-form-input>\r\n      <!-- Fim Input Nome -->\r\n\r\n      <!-- Inicio Rating -->\r\n      <label for=\"classifique\">O Quanto Você Gostou do Filme?</label>\r\n      <b-form-rating\r\n        id=\"classifique\"\r\n        v-model=\"filme.classifique\"\r\n        type=\"number\"\r\n        variant=\"warning\"\r\n        class=\"mb-2\"\r\n      ></b-form-rating>\r\n      <!-- Fim Rating -->\r\n\r\n      <!-- Inicio Textarea -->\r\n      <label for=\"avalie\">Avalie o Filme?</label>\r\n      <b-form-textarea\r\n        id=\"avalie\"\r\n        v-model=\"filme.avalie\"\r\n        type=\"text\"\r\n        placeholder=\"O que você achou desse filme?\"\r\n        required\r\n        rows=\"6\"\r\n        class=\"mb-2\"\r\n      ></b-form-textarea>\r\n      <!-- Fim Textarea -->\r\n\r\n      <!-- Inicio Button -->\r\n      <b-button type=\"submit\" variant=\"outline-primary mt-2\">Salvar</b-button>\r\n      <!-- Fim Button -->\r\n    </b-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport filmeService from \"../services/filmeService\";\r\n\r\nexport default {\r\n  name: \"CadastrarFilme\",\r\n\r\n  data() {\r\n    return {\r\n      filme: {\r\n        nome: \"\",\r\n        classifique: null,\r\n        avalie: \"\",\r\n      },\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    saveFilme() {\r\n      filmeService\r\n        .saveFilme(this.filme)\r\n        .then(() => alert(\"Filme Adicionado Com Sucesso!\"));\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/views"}]}